{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "User",
  "type": "object",
  "properties": {
    "birthdate": {
      "type": "string",
      "format": "date-time"
    },
    "created_time": {
      "type": "string",
      "format": "date-time"
    },
    "display_name": {
      "type": "string"
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "favorite_color": {
      "type": "string",
      "pattern": "^#[0-9a-fA-F]{6}$"
    },
    "photo_url": {
      "type": "string",
      "format": "uri"
    },
    "uid": {
      "type": "string"
    }
  },
  "required": ["birthdate", "created_time", "display_name", "email", "uid"]
}
